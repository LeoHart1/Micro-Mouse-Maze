class Node: 
    def __init__(self, data):
        self.data = data
        self.children = []
    
    def add_child(self, child):
       self.children.append(child)
        
    def print_tree(self, level=0):
        print(" " * level + str(self.data))
        for child in self.children:
            child.print_tree(level + 1)
            
node00 = Node("0,0")
